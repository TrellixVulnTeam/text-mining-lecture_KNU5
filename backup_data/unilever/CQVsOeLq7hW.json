 null, "edge_media_to_tagged_user": {"edges": []}, "edge_media_to_caption": {"edges": [{"node": {"text": "Meet Thierry Angazika, a Hygiene Community Worker,  who is assisting refugees with essential Covid-19 hygiene messages in DRC (UNHCR)\n\n\u201cI wake up every day at 5am. I raise awareness on water and hygiene issues through door-to-door visits. I inspect water points to monitor how people wash their hands and check if the handwashing stations have enough water. With the bicycle provided by UNHCR, I pedal through the camp and can refill the handwashing stations three to five times a day. I really love being a hygiene promoter because it is for my health and also the health of my brothers and sisters. Through this, I serve my community, my parents and my children because I begin good hygiene practices at home.\u201d\n\nThierry is a refugee. \n\nTap the link in our bio to read more.\n\n#WorldRefugeeDay #RefugeeDay #WithRefugees\n\nImage Thierry: Pic credit: UNHCR"}}]}, "can_see_insights_as_brand": false, "caption_is_edited": false, "has_ranked_comments": false, "like_and_view_counts_disabled": false, "edge_media_to_parent_comment": {"count": 6, "page_info": {"has_next_page": false, "end_cursor": null}, "edges": [{"node": {"id": "18073631776287135", "text": "\u2764\ufe0f\u2764\ufe0f\u2764\ufe0f\u2764\ufe0f\u2764\ufe0f\u2764\ufe0f\u2764\ufe0f", "created_at": 1624186910, "did_report_as_spam": false, "owner": {"id": "1964570037", "is_verified": false, "profile_pic_url": "https://scontent-zrh1-1.cdninstagram.com/v/t51.2885-19/s150x150/242389517_195052599263154_1999927727573889061_n.jpg?_nc_ht=scontent-zrh1-1.cdninstagram.com&_nc_ohc=3OZU1BoWIdMAX-BSuYE&edm=AABBvjUBAAAA&ccb=7-4&oh=37a667900c5bb6aac786e14d2131b0b4&oe=6151B043&_nc_sid=83d603", "username": "asumanyeniceri"}, "viewer_has_liked": false, "edge_liked_by": {"count": 1}, "is_restricted_pending": false, "edge_threaded_comments": {"count": 0, "page_info": {"has_next_page": false, "end_cursor": null}, "edges": []}}